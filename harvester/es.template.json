{
  "mappings": {
    "_default_": {
      "_all": {
        "enable": false
      },
      "_source": {
        "enable": false
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 255,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "_body": {
          "type": "keyword",
          "store": true,
          "doc_values": false
        },
        "_tag": {
          "type": "keyword",
          "doc_values": false
        },
        "_timestamp": {
          "type": "long",
          "doc_values": false
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.mapping.total_fields.limit": 5000,
    "index.refresh_interval": "90s",
    "index.number_of_shards" : 4,
    "index.number_of_replicas" : 1
  },
  "template": "ari.*"
}